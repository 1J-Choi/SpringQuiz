<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.quiz.weather_history.mapper.WeatherHistoryMapper">
 	<!-- weatherList에서 전체 날씨 데이터 받아오기 -->
 	<select id="selectWeatherHistroyAll" 
 	resultType="com.quiz.weather_history.domain.WeatherHistory">
 		SELECT
 			`id`
			, `date`
			, `weather`
			, `temperatures`
			, `precipitation`
			, `microDust`
			, `windSpeed`
			, `createdAt`
			, `updatedAt`
 		FROM
 			`weather_history`
 	</select>
 </mapper>